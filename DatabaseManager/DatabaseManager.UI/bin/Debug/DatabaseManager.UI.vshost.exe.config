<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
    </startup>
    <oracle.manageddataaccess.client>
        <version number="*">
            <dataSources>
                <dataSource alias="MyConnection"
                    descriptor="(DESCRIPTION=(ADDRESS=(PROTOCOL=tcp)(HOST=localhost)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=XE))) "/>
            </dataSources>
        </version>
    </oracle.manageddataaccess.client>
  <connectionStrings>
    <add name="MyDB" providerName="Oracle.DataAccess.Client" connectionString="Data Source=MyConnection;User Id=SUPERMARKETS;Password=123456;"/>
  </connectionStrings>
</configuration>